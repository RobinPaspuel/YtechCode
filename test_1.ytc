# This is the first program written in Ytech Code

def fizz_buzz(num_iter){
    for i=1:num_iter{
        if(i%3 == 0 and i%5 ==0){
            print("fizzbuzz")
        }
        elif i%3 == 0 {
            print("fizz")
        } 
        elif i%5 ==0 {
            print("buzz")
        }
        else{
            print(i)
        }
    }
}

fizz_buzz(100)